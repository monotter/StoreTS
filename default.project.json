{
	"name": "storets",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "StoreTS": {
                "$path": "out/StoreTS.lua",
                "Include": {
                    "$path": "include",
                    "node_modules": {
                        "$path": "node_modules/@rbxts"
                    }
                }
            }
        }
	}
}
